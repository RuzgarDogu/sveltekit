{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@components": ["src/components"],
			"@config": ["src/config.js"],
			"@utils": ["src/utils"],
			"@models": ["src/models"],
			"@": ["src"],
		}
	},
	"include": [
		"src/**/*.js",
		"src/**/*.svelte"
	],
	"exclude": [
		"tasks/*.js"
	],
	"extends": "./.svelte-kit/tsconfig.json"
}